
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Athlete {
  id        Int        @id @default(autoincrement())
  nom       String
  sport     String
  paiements Paiement[]
}

model Paiement {
  id        Int     @id @default(autoincrement())
  mois      Int
  paye      Boolean
  athleteId Int
  athlete   Athlete @relation(fields: [athleteId], references: [id], onDelete: Cascade)
}
